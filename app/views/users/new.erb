<%= form_for @user do |f| %>
  <%= f.label :name  %>
  <%= f.text_field :name %>
  <%= f.label :username  %>
  <%= f.text_field :username %>
  <%= f.label :password  %>
  <%= f.password_field :password  %>
  <%= f.label "Confirm Password"  %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit%>
<%end%>
